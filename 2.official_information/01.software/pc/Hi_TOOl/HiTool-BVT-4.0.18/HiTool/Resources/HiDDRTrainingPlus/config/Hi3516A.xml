			<reg baseaddr="0x20118000" offset="0x64" bit="17,21" type="read">
				<list value="0,0" display="34ohm" />
				<list value="1,0" display="40ohm" />
			</reg>
			<reg baseaddr="0x20111000" offset="0x210" bit="0-16,17,21" type="write">
				<list value="0x0012,0,0" display="34ohm" />
