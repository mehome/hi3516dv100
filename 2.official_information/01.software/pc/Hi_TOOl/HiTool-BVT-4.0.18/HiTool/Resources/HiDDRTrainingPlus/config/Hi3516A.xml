				<list value="1,1,0" display="40ohm" />
			</reg>
			<reg baseaddr="0x20111000" offset="0x210" bit="0-16,18,22,25"
				type="write">
				<list value="0x0012,0,0,0" display="Disable" />
				<list value="0x0012,1,0,0" display="60ohm" />
				<list value="0x0012,0,1,0" display="120ohm" />
				<list value="0x0012,1,1,0" display="40ohm" />
			</reg>
			<reg baseaddr="0x20111000" offset="0x218" bit="0" type="write"
				value="1" />
			<reg baseaddr="0x20111000" offset="0x00c" bit="0" type="write"
				value="1" />
		</DqDQSOdtDram>

		<VrefPhy baseaddr="0x20118000" offset="0xb4" bit="0-31">
			<list value="0x0" display="40%*VDDIO" />
