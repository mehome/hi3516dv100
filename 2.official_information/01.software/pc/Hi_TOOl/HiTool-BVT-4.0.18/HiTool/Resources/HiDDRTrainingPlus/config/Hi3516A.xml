				<tzqinit baseaddr="0x20111000" offset="0x8c" bit="12-21" />
				<taond baseaddr="0x20111000" offset="0x8c" bit="8-11" />
				<txard baseaddr="0x20111000" offset="0x8c" bit="4-7" />
				<trtp baseaddr="0x20111000" offset="0x8c" bit="0-3" />

				<tmod baseaddr="0x20111000" offset="0x90" bit="20-24" />
				<twlo baseaddr="0x20111000" offset="0x90" bit="16-19" />
